{
  "name": "appbase",
  "version": "1.0.0",
  "description": "app base",
  "main": "src/main.js",
  "scripts": {
    "start": "electron .",
    "package-win": "electron-packager ./ appbase --overwrite --platform=win32 --arch=x64 --app-version=1.0.0 --icon=assets/app.ico --out=dist --asar --ignore=node_modules/electron-* --ignore=node_modules/.bin --ignore=.git --ignore=dist",
    "package-mac": "electron-packager ./ appbase --overwrite --platform=darwin --arch=x64 --app-version=1.0.0 --icon=assets/app.ico --out=dist --asar --ignore=node_modules/electron-* --ignore=node_modules/.bin --ignore=.git --ignore=dist",
    "package-linux": "electron-packager ./ appbase --overwrite --platform=linux --arch=x64 --app-version=1.0.0 --icon=assets/app.ico --out=dist --asar --ignore=node_modules/electron-* --ignore=node_modules/.bin --ignore=.git --ignore=dist",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "jackjcd",
  "license": "ISC",
  "devDependencies": {
    "electron": "^15.5.5",
    "electron-packager": "^14.2.1"
  },
  "dependencies": {}
}
